{
    // See https://go.microsoft.com/fwlink/?LinkId=733558
    // for the documentation about the tasks.json format
    "version": "0.1.0",
    "command": "npm",
    "isShellCommand": true,
    "args": [
        "--silent",
        "run"
    ],
    "tasks": [
        {
            "taskName": "build",
            "problemMatcher": "$tsc"
        },
        {
            "taskName": "watch",
            "isBackground": true,
            "problemMatcher": "$tsc-watch"
        },
        {
            "taskName": "webpack"
        }
    ]
}